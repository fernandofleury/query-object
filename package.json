{
  "name": "query-object",
  "version": "2.0.3",
  "description": "A lightweight lib to work with query strings (< 1kb uglified)",
  "main": "./dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "prepublish": "webpack",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandofleury/query-object.git"
  },
  "keywords": [
    "query",
    "querystrings",
    "querystring",
    "query",
    "string",
    "url",
    "url-manipulation",
    "manipulation",
    "params",
    "parameters",
    "qs"
  ],
  "author": "Fernando Fleury <fernandofleuryg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fernandofleury/query-object/issues"
  },
  "homepage": "https://github.com/fernandofleury/query-object",
  "devDependencies": {
    "commitizen": "^2.9.6",
    "condition-circle": "^1.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "semantic-release": "^8.2.0",
    "standard": "^10.0.3",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.8.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
